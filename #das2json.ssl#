: Das2json =
  XML Spaces _end

: XML =
  Spaces "<" Stuff
  [
    | ">": Content "</" Stuff ">"
    | "/>": 
    | *: Stuff
  ]

: Content =
  <<
    Spaces
    [*
      | "</": _break
      | "<": XML
      | *: Stuff
    ]
  >>

: Attributes =
  <<
    [*
      | "style=": . String
      | ">": _break
      | "/>": _break
      | eof: _break
      | *: .
    ]
  >>

: Stuff =
  <<
    [*
      | ">": _break
      | "<": _break
      | "/>": _break
      | eof: _break
      | *: .
    ]
  >>

: Spaces =
  <<
    [*
      | " ": .
      | "\t": .
      | "\n": .
      | eof: _break
      | *: _break
    ]
  >>

: String =
  "\"" NotDquotes "\""

: NotDquotes =
  <<
    [*
      | "\"": _break
      | *: .
    ]
  >>
  