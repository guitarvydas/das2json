: Das2json =
  XML Spaces _end

: XML =
  Spaces "<" Stuff
  [
    | ">": Content "</" Stuff ">"
    | "/>": 
  ]

: Content =
  <<<
    Spaces
    [*
      | "</": _break
      | "<": XML
      | *: Stuff
    ]
  >>>

: Attributes =
  <<<
    [*
      | ">": _break
      | "/>": _break
      | _end: _break
      | *: Stuff
    ]
  >>>

: Stuff =
  <<<
    [*
      | ">": _break
      | "<": _break
      | "/>": _break
      | _end: _break
      | *: .
    ]
  >>>

: Spaces =
  <<<
    [*
      | " ": .
      | "\t": .
      | "\n": .
      | *: _break
    ]
  >>>

: String =
  "\"" NotDquotes "\""

: NotDquotes =
  <<<
    [*
      | "\"": _break
      | *: .
    ]
  >>>
  