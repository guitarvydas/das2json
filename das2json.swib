: Das2json @=
  _trace "@0"
  XML Spaces _end

: XML ^=
  Spaces "<" Name Stuff
  [
    | ">": Content "</" Stuff ">"
    | "/>": 
  ]

: Content ^=
  <<<
    Spaces
    [*
      | "</": _break
      | "<mxGeometry ":  mxGeometry
      | "<": XML
      | *: Stuff
    ]
  >>>

: mxGeometry @= XML

: Attributes ^=
  <<<
    [*
      | "style=": Style
      | ">": _break
      | "/>": _break
      | _end: _break
      | *: Stuff
    ]
  >>>

: Style @= "style=" String

: Name ^=
  <<<
    [*
      | " ": _break
      | "\t": _break
      | "\n": _break
      | ">": _break
      | "<": _break
      | "/>": _break
      | _end: _break
      | *: .
    ]
  >>>

: Stuff ^=
  <<<
    [*
      | ">": _break
      | "<": _break
      | "/>": _break
      | _end: _break
      | *: .
    ]
  >>>

: Spaces ^=
  <<<
    [*
      | " ": .
      | "\t": .
      | "\n": .
      | *: _break
    ]
  >>>

: String ^=
  "\"" NotDquotes "\""

: NotDquotes ^=
  <<<
    [*
      | "\"": _break
      | *: .
    ]
  >>>

@ Das2json = _return_value
@ mxGeometry = _ignore_value
@ Style = _ignore_value
